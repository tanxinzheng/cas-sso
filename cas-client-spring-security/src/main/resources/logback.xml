<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>java-spring-security-cas-client-demo  %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <!-- logger name="org.jasig.cas" level="TRACE" />
  <logger name="org.springframework.security" level="TRACE" /-->

  <root level="DEBUG">
    <appender-ref ref="STDOUT" />
  </root>

</configuration>
